# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page

GET		/getEmployees						@controllers.Application.getEmployees()			
POST	/getEmpById							@controllers.Application.getEmpById()
POST	/getEmpByDep						@controllers.Application.getEmpByDep()
POST	/insertEmployee							@controllers.Application.insertEmployee()
POST	/updateEmployee							@controllers.Application.updateEmployee()



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
